    "@graphql-codegen/typescript-react-apollo": "^1.12.2",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@types/uuid": "^7.0.2",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-upload-client": "^13.0.0",
    "axios": "^0.19.2",
    "core-js": "3",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-config": "^3.0.0-alpha.17",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "next": "^9.3.3",
    "next-cookies": "^2.0.3",
    "next-fonts": "^0.19.0",
    "next-with-apollo": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^4.9.8",
    "styled-components": "^4.1.3",
    "styled-reset": "^4.0.8",
    "unstated-next": "^1.1.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@graphql-codegen/add": "^1.12.2",
    "@graphql-codegen/cli": "^1.12.2",
