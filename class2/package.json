{
  "name": "rws_frontend",
  "license": "rws",
  "version": "1.0.0",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "next build",
    "prod": "next start -p 3001",
    "stage": "next start -p 3001",
    "dev": "next -p 3001",
    "apollo:check": "apollo client:check",
    "getSchema": "graphql get-schema",
    "generate": "graphql-codegen",
    "storybook": "start-storybook",
    "lint": "tsc && eslint .",
    "docs": "npm run test && npm run docs:typedoc && npm run docs:storybook",
    "docs:storybook": "build-storybook -c .storybook -o docs/uis",
    "docs:typedoc": "typedoc --out docs/apis --plugin typedoc-plugin-external-module-name src",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
